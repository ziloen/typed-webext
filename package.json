{
  "name": "typed-webext",
  "version": "0.6.6",
  "description": "Type safe web extension api (partial)",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/ziloen/typed-webext",
  "scripts": {
    "build": "tsup",
    "prepack": "npm run build",
    "release": "bumpp --commit --no-push && npm publish"
  },
  "keywords": [
    "browser",
    "extension",
    "typescript"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./storage": "./dist/storage.js",
    "./message": "./dist/message.js",
    "./stream": "./dist/stream.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/webextension-polyfill": "^0.10.7",
    "serialize-error": "<12",
    "type-fest": "^4.12.0",
    "webextension-polyfill": ">=0.10.0 <=1.0.0"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@ziloen/eslint-config": "^0.1.21",
    "bumpp": "^9.4.0",
    "eslint": "^8.57.0",
    "ts-lib-enhance": "^1.0.17",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  },
  "peerDependencies": {
    "serialize-error": "<12",
    "webextension-polyfill": ">=0.10.0 <=1.0.0"
  }
